{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "browsjobdatafact"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureDatabricks1_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks1'"
		},
		"LS_azuresql_transactionalDB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_azuresql_transactionalDB'"
		},
		"LS_blob_transaction_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_blob_transaction'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://externalbrowsjob.dfs.core.windows.net/"
		},
		"AzureDatabricks1_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1223-152044-ubibxsbf"
		},
		"HttpServer1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/suryaphoto416-cloud/azurepocretail/refs/heads/main/customer.json"
		},
		"LS_azuresql_transactionalDB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "retailproj.database.windows.net"
		},
		"LS_azuresql_transactionalDB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "retailprojdb"
		},
		"LS_azuresql_transactionalDB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admin411"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "browsjobdatafact_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureDatabricks1_accessToken": {
						"value": "[parameters('AzureDatabricks1_accessToken')]"
					},
					"LS_azuresql_transactionalDB_password": {
						"value": "[parameters('LS_azuresql_transactionalDB_password')]"
					},
					"LS_blob_transaction_connectionString": {
						"value": "[parameters('LS_blob_transaction_connectionString')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDatabricks1_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks1_properties_typeProperties_existingClusterId')]"
					},
					"HttpServer1_properties_typeProperties_url": {
						"value": "[parameters('HttpServer1_properties_typeProperties_url')]"
					},
					"LS_azuresql_transactionalDB_properties_typeProperties_server": {
						"value": "[parameters('LS_azuresql_transactionalDB_properties_typeProperties_server')]"
					},
					"LS_azuresql_transactionalDB_properties_typeProperties_database": {
						"value": "[parameters('LS_azuresql_transactionalDB_properties_typeProperties_database')]"
					},
					"LS_azuresql_transactionalDB_properties_typeProperties_userName": {
						"value": "[parameters('LS_azuresql_transactionalDB_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}